{
  "name": "ethical-jobs-redux",
  "version": "0.2.8",
  "author": "Andrew McLagan",
  "description": "EthicalJobs.com.au redux utility package",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib/* && NODE_ENV=production gulp build",
