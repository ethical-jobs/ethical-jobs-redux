{
  "name": "ethical-jobs-redux",
  "version": "0.1.0",
  "author": "Andrew McLagan",
  "description": "EthicalJobs.com.au redux utility package",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:umd": "BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/ethical-jobs-redux.js",
    "build:umd:min": "BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/ethical-jobs-redux.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run clean && npm run build && npm run test",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "NODE_PATH=./src jest",
    "test-watch": "NODE_PATH=./src jest --watch"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/__tests__/_fixtures.js"
    ]
  },
  "browser": "dist/ethical-jobs-redux.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "peerDependencies": {
    "axios": "^0.15.3",
    "immutable": "^3.8.1",
    "redux": "^3.6.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel": "6.3.13",
    "babel-cli": "^6.18.0",
    "babel-core": "6.1.18",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "3.5.0",
    "eslint-config-react-app": "0.2.1",
    "eslint-plugin-flowtype": "2.18.1",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "immutable": "^3.8.1",
    "jest-cli": "^18.0.0",
    "redux": "^3.6.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "webpack": "1.12.9"
  },
  "dependencies": {
    "ethical-jobs-sdk": "^0.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethical-jobs/ethical-jobs-redux.git"
  },
  "bugs": {
    "url": "https://github.com/ethical-jobs/ethical-jobs-redux/issues"
  },
  "homepage": "https://github.com/ethical-jobs/ethical-jobs-redux",
  "keywords": [
    "ethicaljobs",
    "employment",
    "environmental",
    "humanitarian",
    "redux"
  ]
}
