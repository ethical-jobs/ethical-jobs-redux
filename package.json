{
  "name": "ethical-jobs-redux",
  "version": "0.1.12",
  "author": "Andrew McLagan",
  "description": "EthicalJobs.com.au redux utility package",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf lib/*",
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "main": "lib/ethical-jobs-redux.js",
  "module": "lib/ethical-jobs-redux.mjs",
  "jsnext:main": "lib/ethical-jobs-redux.mjs",
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babelrc-rollup": "^3.0.0",
    "jest": "^19.0.2",
    "reselect": "^3.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "unfetch": "^2.1.2",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "ethical-jobs-sdk": "^0.1.24",
    "immutable": "4.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethical-jobs/ethical-jobs-redux.git"
  },
  "bugs": {
    "url": "https://github.com/ethical-jobs/ethical-jobs-redux/issues"
  },
  "homepage": "https://github.com/ethical-jobs/ethical-jobs-redux",
  "keywords": [
    "ethicaljobs",
    "employment",
    "environmental",
    "humanitarian",
    "redux"
  ],
  "xo": {
    "parser": "babel-eslint"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/__tests__/_fixtures.js"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/testing/testEnvironment.js"
  }
}
